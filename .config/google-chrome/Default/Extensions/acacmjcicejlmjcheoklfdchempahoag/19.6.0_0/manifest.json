{
   "author": "Lauri Rooden",
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "48": "img/icon-48.png"
      },
      "default_popup": "options.html?p"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "description": "Fast JSON viewer - highlights, shows items count/size, handles large files",
   "icons": {
      "128": "img/icon-128.png",
      "32": "img/icon-32.png",
      "48": "img/icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "JSON Lite",
   "options_page": "options.html",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "contextMenus", "storage", "tabs", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "19.6.0"
}
